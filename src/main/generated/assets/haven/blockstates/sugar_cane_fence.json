{
  "multipart": [
    {
      "apply": {
        "model": "haven:block/sugar_cane_fence_post"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "haven:block/sugar_cane_fence_side_north",
        "uvlock": true
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "haven:block/sugar_cane_fence_side_east",
        "y": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "haven:block/sugar_cane_fence_side_south",
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "haven:block/sugar_cane_fence_side_west",
        "y": 270,
        "uvlock": true
      }
    }
  ]
}