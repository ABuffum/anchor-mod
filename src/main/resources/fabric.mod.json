{
  "schemaVersion": 1,
  "id": "haven",
  "version": "${version}",

  "name": "Haven Pack",
  "description": "Mod Suite for the Haven SMP. Uses assets from the Squirting Syringe Mod [https://github.com/ImExoOdeex/squirtingsyringe]",
  "authors": [
    "mousewich", "knife_moth", "swordcats", "madkiska", "plushieknight"
  ],
  "contact": {
    "homepage": "mousewich.fun",
    "sources": ""
  },

  "icon": "assets/anchors-icon.png",
  
  "accessWidener" : "havenmod.accesswidener",

  "environment": "*",
  "entrypoints": {
    "main": [
	  "fun.mousewich.ModBase"
    ],
    "client" : [
	  "fun.mousewich.ModClient"
    ],
    "fabric-datagen": [
      "fun.mousewich.gen.data.ModDatagen"
    ]
  },
  "mixins": [
    "fun.mousewich.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": "1.17.x",
    "java": ">=16"
  },
  "suggests": {
    "another-mod": "*"
  }
}
